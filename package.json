    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-virtual-scroll-list": "^1.4.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "less": "^4.1.2",
    "less-loader": "^6.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
